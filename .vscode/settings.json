{
  "editor.formatOnSave": true,
  "files.associations": {
    "typeinfo": "c",
    "errno.h": "c",
    "unistd.h": "c",
    "global.h": "c",
    "string.h": "c",
    "stdio.h": "c",
    "ctype.h": "c",
    "stdlib.h": "c",
    "JARAM_~1.C": "cpp"
  },
  "code-runner.executorMap": {
    "c": "cd $dir && make clean && make && ./main && make clean",
    "h": "cd $dir && make clean && make && ./main && make clean"
  },
  "code-runner.clearPreviousOutput": false,
  // "code-runner.preserveFocus": true,
  "code-runner.runInTerminal": true
}
